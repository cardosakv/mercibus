{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "ServiceName": "auth-service",
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ]
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "catalog-service",
      "DownstreamPathTemplate": "/api/brands/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/brands/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Patch",
        "Delete"
      ]
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "catalog-service",
      "DownstreamPathTemplate": "/api/categories/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/categories/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Patch",
        "Delete"
      ]
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "catalog-service",
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/products/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Patch",
        "Delete"
      ]
    },
    {
      "UseServiceDiscovery": true,
      "UseWebSocket": true,
      "ServiceName": "order-service",
      "DownstreamPathTemplate": "/api/orders/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/orders/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Patch",
        "Delete"
      ]
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "order-service",
      "DownstreamPathTemplate": "/order-hub/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/order-hub/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post"
      ]
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "consul",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}
